{
  "expo": {
    "name": "Urban Lens",
    "slug": "urban-lens",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "urbanlens",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.baronn0204.urbanlens",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.baronn0204.urbanlens",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBQ5WXgXLWb7tmIkzdkOyj18QA2DzDlOqU"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-png.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FE7474"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map."
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f9d10612-29c4-4576-a7cc-7c1ad9a82196"
      }
    },
    "owner": "baronn0204",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f9d10612-29c4-4576-a7cc-7c1ad9a82196"
    }
  }
}
